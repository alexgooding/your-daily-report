# Default settings
defaults
auth    on
tls    on
tls_trust_file    /etc/ssl/certs/ca-certificates.crt
logfile ~/.msmtsp.log
# service account
account    service
host       smtp.office365.com
port       587
from       your-daily-report-service@outlook.com
user       your-daily-report-service@outlook.com
password   your-daily-report

account default : service
